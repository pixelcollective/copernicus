{
  "name": "tinypixel/copernicus",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "A WordPress editor extension framework",
  "autoload": {
    "psr-4": {
      "TinyPixel\\Copernicus\\": ["src/tiny-pixel/copernicus/src"],
      "Copernicus\\App\\": ["app"]
    },
    "files": [
      "src/tiny-pixel/copernicus/helpers.php"
    ]
  },
  "require": {
    "php": "^7.1.3",
    "illuminate/cache": "^5.8",
    "illuminate/config": "^5.8",
    "illuminate/console": "^5.8",
    "illuminate/container": "^5.8",
    "illuminate/contracts": "^5.8",
    "illuminate/events": "^5.8",
    "illuminate/filesystem": "^5.8",
    "illuminate/support": "^5.8",
    "illuminate/view": "^5.8",
    "league/flysystem": "^1.0",
    "roots/support": "dev-master",
    "symfony/var-dumper": "^4.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
