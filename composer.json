{
  "name": "tinypixel/blockmodules",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "Modular Block Building",
  "homepage": "https://tinypixel.dev",
  "keywords": [
    "wordpress",
    "plugin",
    "boilerplate"
  ],
  "autoload": {
    "psr-4": {
      "BlockModules\\": ["src/", "app/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BlockModules\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.1",
    "composer/installers": "^1.0",
    "roots/acorn": "dev-master#3e545cfb2f0b637118249202ab5f0707ece3a4cd",
    "roots/support": "dev-master",
    "illuminate/support": "^5.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "squizlabs/php_codesniffer": "^3.3",
    "symfony/var-dumper": "^4.2"
  },
  "scripts": {
    "lint": "phpcs",
    "test": "echo 'The road to hell is paved with good intentions.'"
  }
}
