{
  "name": "tinypixel/copernicus",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "A WordPress editor extension framework",
  "autoload": {
    "psr-4": {
      "TinyPixel\\Copernicus\\": ["src/tiny-pixel/copernicus/src"],
      "Copernicus\\App\\": ["app"]
    }
  },
  "require": {
    "php": "^7.1.3",
    "roots/acorn": "dev-master",
    "roots/support": "dev-master"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
