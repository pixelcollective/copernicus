{
  "name": "tinypixel/base",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "A slim Acorn-powered framework for the plugin space.",
  "autoload": {
    "psr-4": {
      "TinyPixel\\Base\\": "src"
    }
  },
  "require": {
    "php": "^7.1.3",
    "roots/acorn": "dev-master",
    "roots/support": "dev-master"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
