{
  "name": "tiny-pixel/copernicus",
  "type": "package",
  "license": "MIT",
  "description": "Prosperous blockmaking toolkit.",
  "autoload": {
    "psr-4": {
      "TinyPixel\\Copernicus\\": "src"
    }
  },
  "require": {
    "php": "^7.1.3",
    "composer/installers": "^1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "bin-dir": "./bin/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": [
      "phpcs"
    ]
  },
  "extra": {
    "acorn": {
      "providers": [
        "TinyPixel\\Copernicus\\Providers\\CopernicusServiceProvider"
      ]
    }
  }
}
